{
  "name": "swc-plugin-barrel",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.12",
  "description": "",
  "keywords": [
    "swc-plugin"
  ],
  "license": "ISC",
  "author": "JW",
  "files": [
    "swc_plugin_barrel.wasm"
  ],
  "main": "swc_plugin_barrel.wasm",
  "scripts": {
    "test": "cargo test",
    "e2e": "pnpm esno ./e2e/scripts.ts",
    "build": "cargo build-wasi --release && cp ./target/wasm32-wasi/release/swc_plugin_barrel.wasm .",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --no-git-tag --snapshot",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "preferUnplugged": true,
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json,yml,yaml}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@aiou/eslint-config": "1.0.2",
    "@changesets/cli": "^2.26.0",
    "@swc/core": "^1.3.95",
    "@types/node": "^20.8.9",
    "cz-emoji": "1.3.2-canary.2",
    "eslint": "8.52.0",
    "esno": "^0.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "typescript": "^4.6.4"
  }
}
